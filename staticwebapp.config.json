{
  "trailingSlash": "never",
    "routes": [
      {
        "route": "/assets/*",
        "headers": {
          "cache-control": "public, max-age=14400"
        }
      },
      {
        "route": "/*",
        "headers": {
          "cache-control": "public, max-age=3600"
        }
      }
    ]
  }
  